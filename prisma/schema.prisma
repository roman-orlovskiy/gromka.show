generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Contact {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())

  name        String
  email       String
  phone       String?
  description String?

  meta        Json?

  @@map("contacts")
}

